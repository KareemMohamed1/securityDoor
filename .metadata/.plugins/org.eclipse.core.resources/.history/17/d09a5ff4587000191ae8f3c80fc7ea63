#include"uart.h"
#include"external_eeprom.h"
#define PASS_SIZE 4
uint8 savedPass[PASS_SIZE];
uint8 recievedPass[PASS_SIZE];
uint8 size;
uint8 PassmatchFlag;
int main (void){
	EEPROM_init();
	UART_init();
	while(1){
		UART_sendArray(recievedPass,PASS_SIZE);
		EEPROM_readArray(savedPass,PASS_SIZE);


	}
}
